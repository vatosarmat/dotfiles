{
  "arrow_const": {
    "prefix": "ac",
    "body": "const ${1} = (${2}) => ${0}"
  },
  "arrow_const_block": {
    "prefix": "ak",
    "body": "const ${1} = (${2}) => {\n\t${0}\n}"
  },
  "arrow_expression": {
    "prefix": "af",
    "body": "(${1}) => ${0}"
  },
  "arrow_expression_block": {
    "prefix": "ab",
    "body": "(${1}) => {\n\t${0}\n}"
  },

  "console.log": {
    "prefix": "cl",
    "body": "console.log(${0})"
  },

  "import": {
    "prefix": "im",
    "body": "import {${0}} from '${1}'"
  },
  "import_type": {
    "prefix": "it",
    "body": "import type {${0}} from '${1}'"
  },
  "import_default": {
    "prefix": "id",
    "body": "import ${0} from '${1}'"
  },
  "import_star": {
    "prefix": "is",
    "body": "import * as ${0} from '${1}'"
  },

  "method_private": {
    "prefix": "mr",
    "body": "private ${1}(${2}){\n\t${0}\n}"
  },
  "method_public": {
    "prefix": "mp",
    "body": "public ${1}(${2}){\n\t${0}\n}"
  },

  "try_catch": {
    "prefix": "tc",
    "body": "try {\n\t{0}\n} catch(${1}) {\n\t${2}\n}"
  }
}
